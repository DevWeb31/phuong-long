# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Stripe
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_...
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...

# Facebook Graph API (optional)
FACEBOOK_APP_ID=your-app-id
FACEBOOK_APP_SECRET=your-app-secret
FACEBOOK_PAGE_ACCESS_TOKEN=your-page-token

# Email (Resend)
RESEND_API_KEY=re_xxx
RESEND_FROM_EMAIL=noreply@phuong-long-vo-dao.com
RESEND_FROM_NAME=Phuong Long Vo Dao
RESEND_SIGNUP_TEMPLATE_ID=tmpl_xxx  # Optionnel: ID du template Resend pour l'inscription
RESEND_EMAIL_CHANGE_TEMPLATE_ID=tmpl_xxx  # Optionnel: ID du template Resend pour le changement d'email
RESEND_CONTACT_TEMPLATE_ID=tmpl_xxx  # Optionnel: ID du template Resend pour le formulaire de contact
RESEND_CONTACT_DEFAULT_EMAIL=contact@phuong-long-vo-dao.com  # Email par défaut si aucun club sélectionné ou club sans email

# Cloudinary (optional)
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=your-cloud-name
CLOUDINARY_API_KEY=your-api-key
CLOUDINARY_API_SECRET=your-api-secret

# Analytics
NEXT_PUBLIC_VERCEL_ANALYTICS_ID=your-analytics-id

# App
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development

# Database
DATABASE_URL=postgresql://...

# Redis (optional)
UPSTASH_REDIS_REST_URL=https://...
UPSTASH_REDIS_REST_TOKEN=...

# Sentry (optional)
SENTRY_DSN=https://...
NEXT_PUBLIC_SENTRY_DSN=https://...

